{
  "name": "@composio/json-schema-to-zod",
  "version": "0.1.18",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.0"
  },
  "peerDependencies": {
    "zod": ">=3.25.76 <4 || >=4.1 <5"
  },
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "build": "tsup",
    "test": "vitest run",
    "typecheck:src": "tsc --noEmit -p ./tsconfig.src.json",
    "typecheck:test": "tsc --noEmit -p ./tsconfig.test.json",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:test"
  },
  "types": "dist/index.d.ts"
}